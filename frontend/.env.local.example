# Super Agent Platform - Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# ============================================
# API Configuration
# ============================================
NEXT_PUBLIC_API_URL=http://localhost:8000

# ============================================
# Supabase Configuration
# ============================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# ============================================
# PortOne Payment (Client-side)
# ============================================
NEXT_PUBLIC_PORTONE_STORE_ID=your-portone-store-id
NEXT_PUBLIC_PORTONE_CHANNEL_KEY=your-portone-channel-key

# ============================================
# Application Settings
# ============================================
NEXT_PUBLIC_APP_NAME=Super Agent Platform
NEXT_PUBLIC_APP_URL=http://localhost:3000
